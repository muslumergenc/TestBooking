<section data-anim-wrap class="masthead -type-3 relative z-5">
    <div class="container">
        <div class="row justify-center">
            <div class="col-xl-12 lg:py-0 lg:px-0">

                <div class="masthead__tabs">
                    <div class="tabs -bookmark js-tabs">
                        <div class="tabs__controls d-flex justify-content-center js-tabs-controls">
                            <div class="card">
                                <button class="tabs__button flex-column  js-tabs-button"
                                        data-tab-target=".-tab-item-3">
                                    <span class="material-symbols-outlined">
                                        king_bed
                                    </span>
                                    Hotels
                                </button>
                            </div>
                        </div>
                        <div class="tabs__content js-tabs-content">
                            <div class="tabs__pane -tab-item-1 is-tab-el-active">
                                <div class="mainSearch">
                                    <div class="search-flight">
                                        <div class="search-flight-content">
                                            <div class="onehotel box" style="display:block">
                                                <div class="row">
                                                    <form method="post" asp-action="Index" class="row">
                                                        <div class="col-12 col-md-12 col-sm-12 col-lg-6 searchMenu-loc  js-form-dd js-liverSearch">
                                                            <div class="search-input-group" data-x-dd-click="searchMenu-loc">
                                                                <div class="input-group">
                                                                    <span class="input-group-text" id="basic-addon1">
                                                                        <span class="material-symbols-outlined">
                                                                            hotel
                                                                        </span>
                                                                    </span>
                                                                    <input id="citysearch" name="quer" placeholder="Otel, Destinasyon" autocomplete="off" type="text" class="form-control js-search js-dd-focus" />
                                                                </div>
                                                            </div>
                                                            <div class="searchMenu-loc__field shadow-2 js-popup-window" data-x-dd="searchMenu-loc" data-x-dd-toggle="-is-active">
                                                                <div class="bg-white  sm:px-0 sm:py-15 rounded-4">
                                                                    <div class="y-gap-5 js-results">
                                                                        <div class="text-14 px-20 pt-10">
                                                                            <strong>Popüler Destinasyonlar</strong>
                                                                        </div>
                                                                        @if (Model != null)
                                                                        {
                                                                            foreach (var city in Model.cityObjectList)
                                                                            {
                                                                                <div>
                                                                                    <button asp-controller="Hotel" asp-action="HotelsSearchPage" asp-route-id="@city.Id" class="-link d-block col-12 text-left rounded-4 px-20 py-15 js-search-option">
                                                                                        <div class="d-flex">
                                                                                            <div class="icon-location-2 text-light-1 text-20 pt-4"></div>
                                                                                            <div class="ml-10">
                                                                                                <div class="text-15 lh-12 js-search-option-target">@city.Name</div>
                                                                                                <div class="text-14 lh-12 text-light-1 mt-5">@city.Name, Turkey</div>

                                                                                            </div>
                                                                                        </div>
                                                                                    </button>
                                                                                </div>
                                                                            }
                                                                        }
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-12 col-md-6 col-sm-12 col-lg-3">
                                                            <div class="search-input-group">
                                                                <div class="input-group border-white rounded-4">
                                                                    <span class="input-group-text" id="basic-addon1">
                                                                        <span class="material-symbols-outlined">date_range</span>
                                                                    </span>
                                                                    <input type="text" autocomplete="off" name="start-date" class="form-control" id="start-date" placeholder="Giriş Tarihi"  />
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-12 col-md-6 col-sm-12 col-lg-3">
                                                            <div class="search-input-group">
                                                                <div class="input-group border-white rounded-4">
                                                                    <span class="input-group-text" id="basic-addon1">
                                                                        <span class="material-symbols-outlined">date_range</span>
                                                                    </span>
                                                                    <input type="text" autocomplete="off" name="end-date" class="form-control" id="end-date" placeholder="Çıkış Tarihi"  />

                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="d-flex justify-content-center">
                                                            <div class="items-center">
                                                                <button type="submit" class="mainSearch__submit">
                                                                    Search
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

